<main class="flex-1 overflow-x-hidden overflow-y-auto bg-gray-50 p-6">
    <div class="max-w-7xl mx-auto">
      <div class="bg-white shadow rounded-lg p-6">
        <h1 class="text-2xl font-bold text-gray-900 mb-6">Add New Item</h1>
        <p class="text-gray-600">Use this placeholder to add your informations</p>
        <div class="space-y-10 divide-y divide-gray-900/10">
          
          
            <div class="grid grid-cols-1 gap-x-8 gap-y-8 pt-10 md:grid-cols-3">
              <div class="px-4 sm:px-0">
                <h2 class="text-base font-semibold leading-7 text-gray-900">Contact Information</h2>
                <p class="mt-1 text-sm leading-6 text-gray-600">Use this placeholder to add your informations</p>
              </div>
              
          
              <form class="bg-white shadow-sm ring-1 ring-gray-900/5 sm:rounded-xl md:col-span-2">
                <div class="px-4 py-6 sm:p-8">

                  <div class="grid max-w-2xl grid-cols-1 gap-x-6 gap-y-8 sm:grid-cols-6">
                    <div class="sm:col-span-3">
                      <label for="first-name" class="block text-sm font-medium leading-6 text-gray-900">First name</label>
                      <div class="mt-2">
                        <input type="text" name="first-name" id="first-name" autocomplete="given-name" class="block w-full rounded-md border-0 py-1.5 text-gray-900 shadow-sm ring-1 ring-inset ring-gray-300 placeholder:text-gray-400 focus:ring-2 focus:ring-inset focus:ring-indigo-600 sm:text-sm sm:leading-6">
                      </div>
                    </div>
          
                    <div class="sm:col-span-3">
                      <label for="last-name" class="block text-sm font-medium leading-6 text-gray-900">Last name</label>
                      <div class="mt-2">
                        <input type="text" name="last-name" id="last-name" autocomplete="family-name" class="block w-full rounded-md border-0 py-1.5 text-gray-900 shadow-sm ring-1 ring-inset ring-gray-300 placeholder:text-gray-400 focus:ring-2 focus:ring-inset focus:ring-indigo-600 sm:text-sm sm:leading-6">
                      </div>
                    </div>

                    <div class="sm:col-span-3 sm:col-start-1">
                      <label for="region" class="block text-sm font-medium leading-6 text-gray-900">Region</label>
                      <div class="mt-2">                      
                        <select name="region" id="region" autocomplete="region" class="block w-full rounded-md border-0 py-1.5 text-gray-900 shadow-sm ring-1 ring-inset ring-gray-300 focus:ring-2 focus:ring-inset focus:ring-indigo-600 sm:max-w-xs sm:text-sm sm:leading-6">
                          <option>Utsang</option>
                          <option>Amdo</option>
                          <option>Kham</option>
                        </select>
                    </div>
                    </div>
          
                    <div class="sm:col-span-3">
                      <label for="province" class="block text-sm font-medium leading-6 text-gray-900"> Province</label>
                      <div class="mt-2">
                        <select name="province" id="province" autocomplete="province" class="block w-full rounded-md border-0 py-1.5 text-gray-900 shadow-sm ring-1 ring-inset ring-gray-300 focus:ring-2 focus:ring-inset focus:ring-indigo-600 sm:max-w-xs sm:text-sm sm:leading-6">
                            <option>TAR</option>
                            <option>Sichuan</option>
                            <option>Yunnan</option>
                            <option>Qinghai</option>
                            <option>Gansu</option>
                          </select>
                    </div>
                    </div>

                    <div class="sm:col-span-3">
                        <label for="prefecture" class="block text-sm font-medium leading-6 text-gray-900">Prefecture</label>
                        <div class="mt-2">
                          <select name="prefecture" id="prefecture" autocomplete="prefecture" class="block w-full rounded-md border-0 py-1.5 text-gray-900 shadow-sm ring-1 ring-inset ring-gray-300 focus:ring-2 focus:ring-inset focus:ring-indigo-600 sm:max-w-xs sm:text-sm sm:leading-6">
                              <option>Add...</option>
                              
                            </select>
                      </div>
                      </div>

                      <div class="sm:col-span-3">
                        <label for="county" class="block text-sm font-medium leading-6 text-gray-900"> County</label>
                        <div class="mt-2">
                          <select name="county" id="county" autocomplete="county" class="block w-full rounded-md border-0 py-1.5 text-gray-900 shadow-sm ring-1 ring-inset ring-gray-300 focus:ring-2 focus:ring-inset focus:ring-indigo-600 sm:max-w-xs sm:text-sm sm:leading-6">
                              <option>Add..</option>
                              
                            </select>
                      </div>
                      </div>

                      <div class="sm:col-span-3">
                        <label for="township" class="block text-sm font-medium leading-6 text-gray-900">Township</label>
                        <div class="mt-2">
                          <select name="township" id="township" autocomplete="township" class="block w-full rounded-md border-0 py-1.5 text-gray-900 shadow-sm ring-1 ring-inset ring-gray-300 focus:ring-2 focus:ring-inset focus:ring-indigo-600 sm:max-w-xs sm:text-sm sm:leading-6">
                              <option>Add..</option>
                              
                            </select>
                      </div>
                      </div>
                  </div>

                  <div class="sm:col-span-full">
                    <label for="about" class="mt-4 block text-sm font-medium leading-6 text-gray-900">Comments</label>
                    <div class="mt-2">
                      <textarea id="about" placeholder="add the information" name="about" rows="3" class="p-4 block w-full rounded-md border-0 py-1.5 text-gray-900 shadow-sm ring-1 ring-inset ring-gray-300 placeholder:text-gray-400 focus:ring-2 focus:ring-inset focus:ring-indigo-600 sm:text-sm sm:leading-6"></textarea>
                    </div>
                  </div>

                <div class="flex items-center justify-end gap-x-6 border-t border-gray-900/10 px-4 py-4 sm:px-8">
                  <button type="button" class="text-sm font-semibold leading-6 text-gray-900">Cancel</button>
                  <button type="submit" class="rounded-md bg-indigo-600 px-3 py-2 text-sm font-semibold text-white shadow-sm hover:bg-indigo-500 focus-visible:outline focus-visible:outline-2 focus-visible:outline-offset-2 focus-visible:outline-indigo-600">Save</button>
                </div>
              </form>
            </div>
          </div>
      </div>
    </div>
  </main>